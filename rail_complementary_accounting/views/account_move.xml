<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="complementary_account_move_form" model="ir.ui.view">
        <field name="name">complementary.account.move.form</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@name='journal_div']" position="after">
                <field name="complementary_entry" attrs="{'readonly':[('state','!=','draft')]}"/>
            </xpath>
        </field>
    </record>
</odoo>